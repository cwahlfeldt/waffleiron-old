#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../touch/3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules/touch/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/3.1.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/touch/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../touch/3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
  ret=$?
fi
exit $ret
