#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../rc/1.2.8/node_modules/rc/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/rc/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../rc/1.2.8/node_modules/rc/cli.js" "$@"
  ret=$?
fi
exit $ret
