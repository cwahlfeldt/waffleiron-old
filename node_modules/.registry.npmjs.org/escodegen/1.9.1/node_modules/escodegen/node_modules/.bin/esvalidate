#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../esprima/3.1.3/node_modules/esprima/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules/esprima/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/3.1.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/esprima/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../esprima/3.1.3/node_modules/esprima/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
