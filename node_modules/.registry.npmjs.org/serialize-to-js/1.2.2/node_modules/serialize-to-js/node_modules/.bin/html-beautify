#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/bin/html-beautify.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../js-beautify/1.10.1/node_modules/js-beautify/js/bin/html-beautify.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/bin/html-beautify.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules/js-beautify/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/1.10.1/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/js-beautify/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../js-beautify/1.10.1/node_modules/js-beautify/js/bin/html-beautify.js" "$@"
