#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/bin/jest.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/bin/jest.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/node_modules" "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/bin/jest.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules/jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/24.8.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/bin/jest.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/jest/node_modules" node  "$basedir/../jest/bin/jest.js" "$@"
  ret=$?
fi
exit $ret
