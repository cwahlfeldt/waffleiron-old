#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/bin/shjs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/bin/shjs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/node_modules" "$basedir/node"  "$basedir/../shelljs/bin/shjs" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/bin/shjs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules/shelljs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/0.8.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/shelljs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/bin/shjs/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/shelljs/node_modules" node  "$basedir/../shelljs/bin/shjs" "$@"
  ret=$?
fi
exit $ret
