#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/node_modules" "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules/nodemon/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/1.19.1/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/nodemon/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/nodemon/node_modules" node  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
fi
exit $ret
