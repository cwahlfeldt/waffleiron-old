#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/bin/tsserver/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/bin/tsserver/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/node_modules" "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/bin/tsserver/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules/typescript/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/3.5.3/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/typescript/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/bin/tsserver/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/bin/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/typescript/node_modules" node  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
fi
exit $ret
