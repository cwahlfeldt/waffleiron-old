#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules/ts-jest/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules/ts-jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/ts-jest/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/ts-jest/node_modules" "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules/ts-jest/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules/ts-jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/24.0.2_jest@24.8.0/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/ts-jest/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/.registry.npmjs.org/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules:/Users/waffles/.dotfiles/Code/Web/node_modules:/Users/waffles/.dotfiles/Code/node_modules:/Users/waffles/.dotfiles/node_modules:/Users/waffles/node_modules:/Users/node_modules:/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/ts-jest/cli.js/node_modules:/Users/waffles/.dotfiles/Code/Web/waffleiron/node_modules/ts-jest/node_modules" node  "$basedir/../ts-jest/cli.js" "$@"
  ret=$?
fi
exit $ret
